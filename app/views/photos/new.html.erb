<h1>New Photo</h1>

<%= form_for @photo, url: user_family_photos_path(@user, @user.family) do |f| %>
  <%= f.label "Name: " %>
  <%= f.text_field :name %><br>
  <br>
  <%= f.label "Photo URL: " %>
  <%= f.text_field :img_url %><br>
  <br>
  <%= f.collection_select(:family_id, [@user.family], :id, :name) %><br>
  <br>
  <%= f.submit "Create Photo" %>
<% end %>
