<h2>Pritchett Family</h2>
<ul>
  <% @people.each do |person| %>
    <% if person.last_name.include?("Pritchett") %>
      <li><%= link_to person.to_s, person_path(person) %></li>
    <% end %>
  <% end %>
</ul>

